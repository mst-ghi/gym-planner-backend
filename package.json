{
  "name": "gym-planner-backend",
  "version": "1.0.0",
  "description": "Gym Planner Project, backend",
  "author": "Mostafa Gholami",
  "scripts": {
    "prebuild": "rimraf dist && npx prisma generate",
    "build": "nest build",
    "check:types": "tsc --pretty --noEmit",
    "debug": "nest start --debug --watch",
    "dev": "nest start --watch",
    "eslint:check": "eslint \"{src,libs}/**/*.ts\"",
    "eslint:fix": "eslint . --ext ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "lint": "npm run eslint:check && npm run format:check",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:seed": "npx prisma db seed",
    "prod": "node dist/main",
    "start": "node dist/src/main",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.22.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "dayjs": "^1.11.13",
    "helmet": "^7.2.0",
    "js-sha3": "^0.9.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.16",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "npx ts-node prisma/seeds/index.ts"
  }
}
